[user]
	name = Jason Templeton
	email = jtempleton@acemetrix.com
[core]
    pager = less -FRXS -x4
    whitespace=fix,-indent-with-non-tab,space-before-tab,trailing-space,cr-at-eol
	excludesfile = /Users/jtempleton/.gitignore
	trustctime = false
[branch "master"]
  rebase = true
[apply]
	whitespace = strip
[github]
	user = jtemplet
	token = fd412aa6f0af1cee9173995a0752232a
[alias]
	co = checkout
    st = status
    ci = commit
    br = branch
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    hist = log --pretty=format:"%h %ad | %s%d [%an]" --graph --date=short
    ls = ls-files
    dump = cat-file -p
    type = cat-file -t
    pom = pull origin master
    po = pull origin
	update = "!git remote update -p"
	up = pull --ff-only -p
[push]
    default = tracking
[remote "jtemplet-dotfiles"]
	url = git@github.com:jtemplet/dotfiles.git
[remote "jtemplet-scripts"]
	url = git@github.com:jtemplet/scripts.git
[remote "github-hinty"]
	url = git@github.com:oshea/hinty.git
[remote "github-tauri"]
	url = git@github.com:jtemplet/Tauri.git
[remote "github-words"]
	url = git@github.com:oshea/mgf.git

[giggle]
	main-window-maximized = false
	main-window-geometry = 1355x687+88+81
	file-view-vpane-position = 628
	history-view-vpane-position = 168
	main-window-view = HistoryView
	file-view-hpane-position = 341
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[mergetool "diffmerge"]
    path = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge
	cmd = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge --merge --result=\"$MERGED\"\n  \"$LOCAL\" \"$BASE\" \"$REMOTE\"
	trustexitcode = false
[merge]
	tool = diffmerge
[diff]
	tool = diffmerge
[difftool "diffmerge"]
	cmd = diffmerge \"$LOCAL\" \"$REMOTE\"
[mergetool]
	keepBackup = false
[pivotal]
	api-token = 7df138110b993d2a0a069f44528a4a47
	full-name = Jason Templeton
	only-mine = true
	project-id = 406407
